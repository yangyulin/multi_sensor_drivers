<launch>

  <arg name="vicon_server" default="192.168.1.162"/>


  <node pkg="vicon" type="vicon" name="vicon" output="screen">
    <param name="vicon_server" value="$(arg vicon_server)"/>
    <param name="calib_files_dir" value="$(find vicon)/calib"/>
    <param name="enable_unlabeled_markers" value="false"/>
  </node>
</launch>
